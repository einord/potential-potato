{
  "compilerOptions": {
    "target": "ESNext",
    // "module": "commonjs",
    "module": "esnext",
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "sourceMap": true,
    "baseUrl": ".",
    "outDir": "dist",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "types": ["vite/client"],
  },
  "include": ["src", "src/vite-env.d.ts"]
}
